<!DOCTYPE html>
<html lang="fr">
<head>
    {% block head %}
        <title>{% block title %}{{vartwig.title}}{% endblock %}</title>
        {{ include('head/head.html.twig') }}
    {% endblock head %}
    {{ encore_entry_link_tags('app') }}
        {% if mcdata is defined %}
                {{ include('microdata/website/post.html.twig') }}
        {%  endif %}
        {% if infowb is defined %}
                {{ include('microdata/website/show.html.twig') }}
        {%  endif %}
        {{ encore_entry_link_tags('app') }}
</head>
<!-- Google tag (gtag.js) -->
<script async src="https://www.googletagmanager.com/gtag/js?id=G-P171X7Q4YQ"></script>
<script>
        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());

        gtag('config', 'G-P171X7Q4YQ');
</script>
<script>
        if ('serviceWorker' in navigator) {
                window.addEventListener('load', function() {
                        navigator.serviceWorker.register('./_service-worker.js').then(function(registration) {
                                // Registration was successful
                                console.log('ServiceWorker registration successful with scope: ', registration.scope);
                        }, function(err) {
                                // registration failed :(
                                console.log('ServiceWorker registration failed: ', err);
                        });
                });
        }
</script>
{%  set agent = app.session.get('agent') %}
<body class="{{ agent=='desk/'? 'wbesitedesk' : 'wbesitemob' }}">
{{ encore_entry_script_tags('app') }}
{% if board is not null %}
        <div class="wb-inf-" data-idwb="{{ board.id }}" data-slugwb="{{ board.slug }}" data-namewb="{{ board.nameboard }}" data-agent="{{ agent }}" data-replacejs="{{ replacejs?'yes':'no' }}"></div>
{% else %}
        {%  set city = app.session.get('city') %}
        <div class="wb-inf-" data-agent="{{ agent }}" data-replacejs="{{ replacejs?'yes':'no' }}"></div>
{% endif %}
{% block menu %}
{% endblock menu %}
{% block core %}
    {% block body %}
    {% endblock body %}
    {% block javascripts %}
    {% endblock %}
{% endblock core %}
</body>
</html>
