<div class="fx-pw">
    <div class="">
        <div class="inf_modul">
            <h2 class="_title til_mdl">Vos modules</h2>
            <div class="content-prov-modul">
                <div class="w100" id="module_1">
                    <div class="inf_modul">
                        <h2 class="_title til_mdl">{{ module.title }}</h2>
                        <div class="fx-content">
                            <div class="content-prov-modul">
                                <div class="prov-modul">
                                    <p>{{ module.desc }}</p>
                                    <div class="_cTm-Wb">
