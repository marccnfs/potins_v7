                                                <div class="state-item" >
                                                    <em>lien :</em>
                                                    <cite class="-ct_b">{{board.module.contactation.linkone }}</cite>
                                                </div>
                                                <a class="stape_order_menu middlegrid stape_bull" href="{{path('process_contact',{'id':board.id}) }}">modifier</a>
                                            {% else %}
                                                <div class="state-item" >
                                                    <h6>pas de module actif</h6>
                                                </div>
                                                <a class="stape_order_menu middlegrid stape_bull" href="{{path('process_contact',{'id':board.id}) }}">creer</a>
                                            {% endif %}
                                        {% else %}
                                            {% if status %}
