                                                    <cite class="-ct_b">activez ce module pour l'utiliser</cite>
                                                </div>
                                                <button id="btnmodule" data-module="{{ module.link }}" class="stape_order_menu middlegrid stape_bull" >installer</button>
                                            {% else %}
                                                <div class="state-item" >
                                                    <h6>Ajouter ce service Ã  mon compte</h6>
                                                    <p>{{ module.price }}</p>
                                                </div>
                                                <a class="stape_order_menu middlegrid stape_bull" href="{{ path('new_cmd_customer',{'md':module.link}) }}">j'ajoute ce service</a>
                                            {% endif %}
                                        {%  endif %}
