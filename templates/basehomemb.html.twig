<!DOCTYPE html>
<html lang="fr">
<head>
    <title>{% block title %}{{vartwig.title}}{% endblock %}</title>
    {{include('aff_potins/head/head.html.twig')}}
    {{ encore_entry_link_tags('app') }}
</head>
<!-- Google tag (gtag.js) -->
<script async src="https://www.googletagmanager.com/gtag/js?id=G-P171X7Q4YQ"></script>
<script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());

    gtag('config', 'G-P171X7Q4YQ');
</script>
<script>
    if ('serviceWorker' in navigator) {
        window.addEventListener('load', function() {
            navigator.serviceWorker.register('./_service-worker.js').then(function(registration) {
                // Registration was successful
                console.log('ServiceWorker registration successful with scope: ', registration.scope);
            }, function(err) {
                // registration failed :(
                console.log('ServiceWorker registration failed: ', err);
            });
        });
    }
</script>
{{ encore_entry_script_tags('app') }}
<body>
<main id="page" class="content-constrain2">
	<header class="HO shadow-large header-app user_menu2">
         <label class="sandw-left" data-menu="menu">
            <span class="_header-main--trigger_icon">
                <i class="_header-main--cross_hatch4 _hatch4"></i>
            </span>
        </label>
        <div class="pos_menu2">
            <div class="container-af-menu" >
                <h1 class="domaine2" id="_desktop_main">Mademoiselle Gingembre</h1>
            </div>
        </div>
        <div id="_desktop_user_info">
            <i class="fa fa-user"></i>
            <ul class="user-info">
                <li>
                    <a class="account" href="" title="Voir mon compte client" rel="nofollow">
                        <span class="hidden-sm-down"></span>
                    </a>
                </li>
                <li>
                    <a class="logout hidden-sm-down" href="" rel="nofollow">
                        <i class="fa fa-user"></i>
                    </a>
                </li>
            </ul>
        </div>
    </header>
      {{ include ('aff_potins/menu/menu_prod.html.twig') }}
     <div role="main">
      	{% block body %}{% endblock %}
    </div>
</main>
{% block javascripts %}
